import random


def matching(a,b):
    num = 0
    if len(a) == len(b) or len(a) < len(b):
        for i in len(a):
            if a[i] == b[i]:
                num = num + 1
    else:
        for i in len(b):
            if a[i] == b[i]:
                num = num + 1
    return num


a = [1,2,3]
b = [2,3,4]

c = [2,2,3]
d = [1,2,4,5,6]
